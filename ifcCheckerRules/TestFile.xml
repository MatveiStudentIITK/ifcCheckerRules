<?xml version="1.0" encoding="utf-8" ?>
<IfcCheckerRules>
	<ifcVersion>Ifc4</ifcVersion>
	<fileSize>256</fileSize>
	<FileNameMask>
		<separator>_</separator>
		<FileNamePlaceholder>
			<order>25</order>
			<type>Тип</type>
			<value>Значение;Значение</value>
		</FileNamePlaceholder>
	</FileNameMask>
	<CategoryElementColors>
		<attributeName>Наименование системы по ПИМ</attributeName>
		<attributeValue>Трубопроводная система: Прочее</attributeValue>
		<Color>
			<r>255</r>
			<g>0</g>
			<b>255</b>
		</Color>
		<ifcClass>IfcPipeSegment</ifcClass>
	</CategoryElementColors>
	<Coordinates>
		<name></name>
		<n></n>
		<e></e>
		<a></a>
		<r></r>
	</Coordinates>
	<AttributesComparison>
		<ifcClass></ifcClass>
		<Attribute>
			<name></name>
			<type></type>
		</Attribute>
		<Comparison>
			<comparisonType></comparisonType>
			<comparisonValue></comparisonValue>
		</Comparison>
	</AttributesComparison>
</IfcCheckerRules>